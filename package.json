{
  "name": "nodejs-backend-architecture",
  "version": "2.0.1",
  "description": "The architecture for nodejs backend application. It is build on top of expressjs using typescript.",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && npm run serve",
    "dev": "nodemon -r dotenv/config  src/Server.ts",
    "serve": "node -r dotenv/config build/Server.js",
    "build": "npm run clean && npm run build-ts && gulp",
    "watch": "npx concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "watch-node": "nodemon -r dotenv/config build/Server.js",
    "clean": "rimraf ./build",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "eslint": "eslint . --ext .js,.ts",
    "upgrade": "npm update --save-dev && npm update --save",
    "test": "jest --forceExit --detectOpenHandles --coverage --verbose",
    "gen-model": "rm -rf ./src/lib/mysql/models && npx sequelize-auto -a ./src/lib/mysql/config.js -o ./src/lib/mysql/models --cp c -l ts -d link-palette-dev -h localhost -u link-palette -p 1111 -x P@ssw0rd -e mysql && npm run replace-uuid-gen && npx eslint --fix src/lib/mysql/models/*.ts",
    "replace-uuid-gen": "replace-in-files --string=\"Sequelize.Sequelize.fn('uuid')\" --replacement=\"DataTypes.UUIDV4\" ./src/.",
    "make:migration": "npx sequelize-cli migration:generate --migrations-path database/migrations --name",
    "make:seed": "npx sequelize-cli seed:generate --seeders-path database/seeders --name",
    "db:migrate": "npx  sequelize-cli db:migrate --migrations-path database/migrations --config database/configs/config.js",
    "db:revert": "npx  sequelize-cli db:migrate:undo --migrations-path database/migrations --config database/configs/config.js",
    "db:revert:all": "npx  sequelize-cli db:migrate:undo:all --migrations-path database/migrations --config database/configs/config.js",
    "db:seed": "npx  sequelize-cli db:seed --seeders-path database/seeders --config database/configs/config.js --seed",
    "db:seed:all": "npx  sequelize-cli db:seed:all --seeders-path database/seeders --config database/configs/config.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/janishar/nodejs-backend-architecture-typescript.git"
  },
  "author": "Janishar Ali",
  "license": "Apache-2.0",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.620.0",
    "@aws-sdk/s3-request-presigner": "^3.620.0",
    "axios": "^1.7.6",
    "base64url": "^3.0.1",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "handlebars": "^4.7.8",
    "joi": "^17.7.0",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.3",
    "moment": "^2.29.4",
    "mongoose": "^6.8.0",
    "mysql2": "^3.10.0",
    "node-cron": "^3.0.3",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.9.14",
    "redis": "^4.5.1",
    "reflect-metadata": "^0.2.1",
    "replace-in-files-cli": "^2.2.0",
    "sequelize": "^6.37.1",
    "sequelize-auto": "^0.8.8",
    "sequelize-typescript": "^2.1.6",
    "unique-names-generator": "^4.7.1",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.2.4",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/lodash": "^4.14.191",
    "@types/module-alias": "^2.0.4",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/newrelic": "^9.4.0",
    "@types/newrelic__winston-enricher": "^0.1.1",
    "@types/node": "^18.11.13",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.15",
    "@types/request": "^2.48.8",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@typescript-eslint/parser": "^5.46.0",
    "colors": "^1.4.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-typescript": "^0.14.0",
    "eslint-plugin-unused-imports": "^4.1.3",
    "gulp": "^5.0.0",
    "gulp-cli": "^3.0.0",
    "gulp-copy": "^4.0.1",
    "jest": "^29.3.1",
    "nodemon": "^2.0.20",
    "prettier": "^3.3.3",
    "sequelize-cli": "^6.6.2",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "overrides": {
    "typeorm": {
      "mongodb": {
        "bson": "5.5.1"
      }
    }
  }
}
